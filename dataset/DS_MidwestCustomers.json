{
	"name": "DS_MidwestCustomers",
	"properties": {
		"linkedServiceName": {
			"referenceName": "BlobStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"_ScrBlobContainer": {
				"type": "string",
				"defaultValue": "customers"
			},
			"_ScrBlobFilename": {
				"type": "string",
				"defaultValue": "mock_customer_contacts_upper_midwest_50.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset()._ScrBlobFilename",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset()._ScrBlobContainer",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "FirstName",
				"type": "String"
			},
			{
				"name": "LastName",
				"type": "String"
			},
			{
				"name": "Email",
				"type": "String"
			},
			{
				"name": "Phone",
				"type": "String"
			},
			{
				"name": "Address",
				"type": "String"
			},
			{
				"name": "City",
				"type": "String"
			},
			{
				"name": "State",
				"type": "String"
			},
			{
				"name": "Zip",
				"type": "String"
			}
		]
	}
}